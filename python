class Friend:
    def __init__(self, name="", age=0):
        self.name = name
        self.age = age

    def greet(self):
        print(f"Hi, I'm {self.name} and I'm {self.age} years old.")


def main():
    my_friend = Friend("emmaculate", 17)
    my_friend.greet()


if __name__ == "__main__":
    main()
