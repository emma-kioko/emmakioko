# PYTHON CODE

class Fraction:
    def __init__(self, num, den):
        self.num = num
        self.den = den

def greater_frac(a, b):
    return a.num * b.den > b.num * a.den

# pass-by-reference & return-by-reference
def largest_fraction(a, b, c):
    if greater_frac(a, b) and greater_frac(a, c):
        return a
    elif greater_frac(b, a) and greater_frac(b, c):
        return b
    else:
        return c

f1 = Fraction(3, 4)
f2 = Fraction(5, 6)
f3 = Fraction(2, 3)

result = largest_fraction(f1, f2, f3)
print(result.num, "/", result.den)
