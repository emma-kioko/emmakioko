class Employee:
    # Parameterized constructor
    def __init__(self, name, age, serviceYear, salary):
        self.name = name
        self.age = age
        self.serviceYear = serviceYear
        self.salary = salary

    # Destructor
    def __del__(self):
        pass   # No special cleanup needed

    # Accessor (getter) methods
    def getName(self):
        return self.name

    def getAge(self):
        return self.age

    def getServiceYear(self):
        return self.serviceYear

    def getSalary(self):
        return self.salary


# Example usage
emp = Employee("Queen Emma", 25, 3, 75000)

print("Name:", emp.getName())
print("Age:", emp.getAge())
print("Service Year:", emp.getServiceYear())
print("Salary:", emp.getSalary())
